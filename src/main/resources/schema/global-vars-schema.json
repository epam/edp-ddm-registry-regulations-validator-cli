{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "dataFactoryBaseUrl": {
      "type": "string",
      "pattern": "^http(s?)\\:\\/\\/[0-9a-zA-Z]([-.\\w]*[0-9a-zA-Z])*(:[0-9]+)*$"
    },
    "supportEmail": {
      "type": "string"
    },
    "themeFile": {
      "type": "string",
      "enum": [
        "white-theme.js",
        "dark-theme.js"
      ]
    }
  },
  "additionalProperties": false,
  "required": [
    "dataFactoryBaseUrl"
  ]
}